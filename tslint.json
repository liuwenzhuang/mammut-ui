{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "interface-name": false,
        "member-access": false,
        "no-console": [true, "log", "error"],
        "no-empty-interface": false,
        "trailing-comma": [true, {
            "multiline": {
                "objects": "always",
                "arrays": "always",
                "functions": "never",
                "typeLiterals": "ignore"
            },
            "esSpecCompliant": true
        }],
        "arrow-parens": false
    },
    "rulesDirectory": []
}
